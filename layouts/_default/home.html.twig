{% extends '_default/page.html.twig' %}

{%- block content ~%}
      <div id="home" class="flex-col">
        {#- blocks ~#}
        {%- for block in page.blocks|default ~%}
        {{- include('blocks/' ~ block.name ~ '.html.twig', ignore_missing = true) ~}}
        {%- endfor ~%}
        {#- content ~#}
        <div id="content" class="markdown dark:markdown-invert max-w-2xl mx-auto px-6 md:px-0 py-6">
          {{ page.content }}
        </div>
      </div>
{%- endblock ~%}